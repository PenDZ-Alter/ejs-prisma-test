{
  "name": "ejs-prisma",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun src/index.ts",
    "migrate:dev": "bun prisma migrate dev --name update",
    "migrate:prod": "bun prisma migrate deploy --name update-prod",
    "migrate:reset": "bun prisma migrate reset",
    "db:generate": "bun prisma generate"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "prisma": "^6.11.1"
  }
}
